//撤回申请
function chehui(){
		$.messager.confirm("消息提示","确定要撤回吗？",function(r){
			if(r){
				$.messager.confirm("消息提示","撤回成功",function(r){
					if(r){
						location.href="${ctx}/pact/index"
					}else{
						location.href="${ctx}/pact/index"
					}
				})
			}
		})
	
}
